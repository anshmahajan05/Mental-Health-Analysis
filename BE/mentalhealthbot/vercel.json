{
    "builds": [
        {
            "src": "mentalhealthbot/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.10",
                "pip": "22.0.2"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "mentalhealthbot/wsgi.py"
        }
    ]
}
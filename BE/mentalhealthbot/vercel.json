{
    "version": 2,
    "builds": [
        {
            "src": "mentalhealthbot/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.12"
            }
        },
        {
            "src": "build.sh",
            "use": "@vercel/static-build"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "mentalhealthbot/wsgi.py"
        }
    ]
}